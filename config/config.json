{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "coveo-integration-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/coveoIntegration/CoveoIntegrationWebPart.js",
          "manifest": "./src/webparts/coveoIntegration/CoveoIntegrationWebPart.manifest.json"
        }
      ]
    },
    "global-search-box-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/globalSearchBox/GlobalSearchBoxApplicationCustomizer.js",
          "manifest": "./src/extensions/globalSearchBox/GlobalSearchBoxApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "CoveoIntegrationWebPartStrings": "lib/webparts/coveoIntegration/loc/{locale}.js",
    "GlobalSearchBoxApplicationCustomizerStrings": "lib/extensions/globalSearchBox/loc/{locale}.js"
  }
}
